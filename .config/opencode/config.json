{
  "$schema": "https://opencode.ai/config.json",
  "theme": "user",
  "model": "anthropic/claude-sonnet-4-20250514",
  "layout": "stretch",
  "keybinds": {
    "input_newline": "alt+j"
  },
  "mode": {},
  "provider": {
    "ollama": {
      "npm": "@ai-sdk/openai-compatible",
      "options": {
        "baseURL": "https://ollama.holdenitdown.net/v1"
      },
      "models": {
        "deepseek-r1:32b": {},
        "devstral": {},
        "qwen3:32b": {},
        "gemma3:27b": {}
      }
    },
    "openrouter": {
      "name": "OpenRouter",
      "models": {
        "moonshotai/kimi-k2": {
          "name": "Kimi K2"
        },
        "moonshotai/kimi-k2:free": {
          "name": "Kimi K2 Free"
        },
        "qwen/qwen3-coder": {
          "name": "Qwen3 Coder"
        },
        "z-ai/glm-4.5": {
          "name": "GLM 4.5"
        },
        "z-ai/glm-4.5-air": {
          "name": "GLM 4.5 Air"
        }
      }
    }
  },
  "mcp": {
    "context7": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@upstash/context7-mcp"
      ],
      "enabled": true
    }
  },
  "instructions": [
    "CONTRIBUTING.md",
    "CLAUDE.md",
    ".cursor/rules/*.md"
  ],
  "share": "disabled",
  "autoshare": false,
  "autoupdate": false
}
